import WRITEPE1;
import WRITE1;
import CLEAR1;
import AND3;
import OR3;
import JUMP0N1;
import JUMPTARGET0;
import CLEAR0;
import SET1;
import READ1;
import INV0;
import TRIGGER1;
import COPY2;
import JUMPADR1;
import AND1;
import OR1;
import IFVALUE4;

MAIN:
	alloc F;
	CLEAR1 F;
	CLEAR0;
	INV0;
	CJUMP0ADR1 LABEL;
	SET1 F;
	JUMPADR1 LABEL;
	global LABEL: JUMPTARGET0;
	READ1 F;
	INV0;

CJUMP0ADR1 ADR:
	local alloc H;
	WRITE1 H;
	CJUMP011N2 H,&ADR;
	
CJUMP011N2 IF, T:
	global alloc ACTUALTARGET[$N];
	local symbol ATMS ACTUALTARGET[-1];
	local symbol TMS T[-1];
	local symbol CLMS &CONTLABEL[0][-1];
	[1:$N] IFVALUE4 IF, TMS[$i], CLMS[$i], ATMS[$i];
	JUMP0N1 ACTUALTARGET;
	global CONTLABEL: JUMPTARGET0;