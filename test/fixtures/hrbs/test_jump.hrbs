import CLEAR1;
import SET1;
import READ1;
import INV0;
import COPY2;
import TRIGGER1;
import SET1;

MAIN:
	alloc F;
	CLEAR1 F;
	JUMP0N1 LABEL;
	SET1 F;
	JUMP0N1 LABEL;
	global LABEL: JUMPTARGET0;
	READ1 F;
	INV0;
	
JUMPTARGET0:
	NAW ADR_EVAL;
	
JUMP0N1 T:
	symbol TM1_MS &T[0][-1];
	symbol ADR_OFS_M1_MS ADR[-1]
	[1:$N] COPY2 TM1_MS[$i], ADR_OFS_M1_MS[$i];
	TRIGGER1 ADR_EVAL;
	

